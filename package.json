  "bugs": {
    "url": "https://github.com/kristycode/PRITY/issues"
  },
  "homepage": "https://github.com/kristycode/PRITY#readme",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.19"
  }
}
