{
  "name": "prity",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristycode/PRITY.git"
  },
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kristycode/PRITY/issues"
  },
  "homepage": "https://github.com/kristycode/PRITY#readme",
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "latest",
    "@material-ui/styles": "^4.10.0",
    "@okta/okta-react": "^1.0.3",
    "@svgr/webpack": "^5.4.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.21",
    "mongoose-bcrypt": "^1.8.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-inlinesvg": "^2.0.0",
    "react-mdl": "^2.1.0",
    "react-router-dom": "^4.3.1",
    "react-spring": "^8.0.27",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.1.1",
